$(function(){if($("#staticDecorator").val()!="1"){$("img.img_lazy").lazyload({threshold:100,effect:"fadeIn"})}$("#filterByHeadCity").find("a").attr("onclick","searchHead('filterByHeadCity', this);");$("#filerByHeadTag").find("a").attr("onclick","searchHead('filerByHeadTag', this);");$("#filterByCity").find("a").attr("onclick","commonSearchEvent('filterByCity', this);");var g=$("#currentPage").val();if(!emptyStr(g)){$("#currentpage"+g).addClass("active")}else{if($("#totalPage").val()>1){$("#currentpage1").addClass("active")}}var e=$("#selectDateWay").val();if(emptyStr($("#selectDate").val())){if(!emptyStr(e)){if(e=="d1"||e=="d2"||e=="d3"||e=="d4"){$("#filterByDate a").removeClass("active");$("#filterByDate").find("a[data-value="+e+"]").addClass("active")}else{commonSearchEvent("filterByDate",null)}}}else{$("#filterByDate a").removeClass("active");$("#view-category-date").addClass("active")}var c=$("#selectDate").val();var f=FormatDate(new Date());if(emptyStr(c)){c=f}$("#view-category-date").datetimepicker({timepicker:false,format:"Y-m-d",minDate:f,value:c,onSelectDate:function(l){if(!emptyStr(l)){commonSearchEvent("selectDate",l)}}});var k=$("#selectTag").val();if(!emptyStr(k)){$("#filterByTag a").removeClass("active");$("#filterByTag").find("a[data-value="+k+"]").addClass("active")}var i=$("#selectType").val();if(!emptyStr(i)){$("#filterByType a").removeClass("active");$("#filterByType").find("a[data-value="+i+"]").addClass("active")}var d=$("#selectCity").val();if(!emptyStr(d)){$("#filterByCity a").removeClass("active");$("#filterByCity").find("a[data-value="+d+"]").addClass("active")}var b=$("#selectOrder").val();if(!emptyStr(b)){$("#filterByOrder a").removeClass("active");$("#filterByOrder").find("a[data-value="+b+"]").addClass("active")}if($("#showMode").val()=="1"){$("#defalutMode").removeClass("active");$("#otherMode").addClass("active")}else{$("#otherMode").removeClass("active");$("#defalutMode").addClass("active")}var h=$("#selectFree").val();var a=$("#selectBag").val();var j=$("#selectDiscount").val();if(h=="1"){$("#selectFree").prop("checked","true");$("#filterByDiscount").css("display","none")}if(a=="1"){$("#selectBag").prop("checked","true")}if(j=="1"){$("#selectDiscount").prop("checked","true");$("#filterByFree").css("display","none")}});function searchEventlist(b){var a="/eventlist.html?f=1&";var c="";if(!emptyStr(b)){c+="eventName="+b+"&"}else{alertWarningTips("请输入搜索内容！");return false}var d=$("#location").text();if(d!="全国"){c+="city="+$("#citySearch").val()+"&"}if(emptyStr(c)){a=a.substr(0,a.length-1)}else{a+=c.substr(0,c.length-1)}window.location.href=a}function searchEventlistButton(){var b="/eventlist.html?f=1&";var c=$("#citySearch").val();var a=$("#searchEventName").val();if(!emptyStr(c)){b+="city="+c+"&"}if(!emptyStr(a)){b+="eventName="+a+"&"}b=b.substring(0,b.length-1);window.location.href=b}function searchHead(b,a){if(b=="filerByHeadDate"){if(emptyStr($(a).attr("data-value"))){window.location.href="/eventlist.html?f=1"}else{window.location.href="/eventlist.html?f=1&d="+$(a).attr("data-value")}}else{if(b=="filerByHeadTag"){if(emptyStr($(a).attr("data-value"))){window.location.href="/eventlist.html?f=1"}else{window.location.href="/eventlist.html?f=1&tag="+$(a).attr("data-value")}}else{if(b=="filterByHeadCity"){if(emptyStr($(a).attr("data-value"))){window.location.href="/eventlist.html?f=1"}else{window.location.href="/eventlist.html?f=1&city="+$(a).attr("data-value")}}}}}function commonSearchEvent(e,i){var b="/eventlist.html?f=1&";var k="";var p=$("#selectTag").val();var n=$("#selectType").val();var h=$("#selectCity").val();var f=$("#selectOrder").val();var d=$("#selectDateWay").val();var g=$("#selectDate").val();var m="";switch(e){case"filterByTag":p=$(i).attr("data-value");break;case"filterByType":n=$(i).attr("data-value");break;case"filterByCity":h=$(i).attr("data-value");break;case"filterByOrder":f=$(i).attr("data-value");break;case"filterByFree":if($(i).val()=="0"||emptyStr($(i).val())){$(i).val(1)}else{$(i).val(0)}break;case"filterByBag":if($("#selectBag").is(":checked")){$("#selectBag").val("1")}else{$("#selectBag").val("0")}break;case"filterByDiscount":if($("#selectDiscount").is(":checked")){$("#selectDiscount").val("1")}else{$("#selectDiscount").val("0")}break;case"filterByMode":if(i.id=="defalutMode"){$("#showMode").val("0")}else{$("#showMode").val("1")}break;case"filterByDate":if(!emptyStr(i)){d=$(i).attr("data-value");g=""}else{d=""}break;case"selectDate":g=i;d="";break;case"filterByPage":m=$(i).attr("data-value");break}if(!emptyStr(d)){k+="d="+d+"&"}if(!emptyStr(g)){g=FormatDate(g);k+="date="+g+"&"}if(!emptyStr(p)){k+="tag="+p+"&"}if(!emptyStr(n)){k+="type="+n+"&"}if(!emptyStr(h)){k+="city="+h+"&"}if(!emptyStr(f)){k+="r="+f+"&"}var l=$("#selectFree").val();var a=$("#selectBag").val();var o=$("#selectDiscount").val();if(l=="1"){k+="free=1&"}if(a=="1"){k+="bag=1&"}if(o=="1"){k+="discount=1&"}var j=$("#searchEventName").val();if(!emptyStr(j)){k+="eventName="+j+"&"}if(!emptyStr(m)){k+="page="+m+"&"}var c=$("#showMode").val();if(!emptyStr(c)){k+="showMode="+c+"&"}if(emptyStr(k)){b=b.substring(0,b.length-1)}else{b+=k.substring(0,k.length-1)}window.location.href=b}function FormatDate(b){var a=new Date(b);return a.getFullYear()+"-"+(a.getMonth()+1)+"-"+a.getDate()};